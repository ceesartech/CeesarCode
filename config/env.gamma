# CeesarCode Gamma (Pre-Production) Environment Configuration
# Used by ./scripts/start-gamma.sh and CI/CD gamma deployment
# This environment mirrors production but uses lower-cost infrastructure

# Environment
APP_ENV=gamma

# Server Configuration
PORT=8080
# Update this with your gamma deployment URL
FRONTEND_ORIGIN=https://gamma.ceesarcode.example.com

# Executor Mode (docker for gamma - sandboxed execution)
EXECUTOR_MODE=docker

# Logging
LOG_LEVEL=info

# Feature Flags
ENABLE_PAIR_PROGRAMMING=true
ENABLE_SYSTEM_DESIGN_AGENT=true
ENABLE_WEB_SEARCH=true

# AI Provider Keys (use separate keys/quotas from prod if possible)
# GEMINI_API_KEY=
# OPENAI_API_KEY=
# ANTHROPIC_API_KEY=
DEFAULT_AI_PROVIDER=gemini

# Gamma-specific Configuration
# Lower resource limits for cost optimization
MAX_CONCURRENT_EXECUTIONS=5
EXECUTION_TIMEOUT_SECONDS=30
MAX_MEMORY_MB=256

