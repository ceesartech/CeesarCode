# CeesarCode Production Environment Configuration
# Used by ./scripts/start-prod.sh and CI/CD production deployment

# Environment
APP_ENV=prod

# Server Configuration
PORT=8080
# Update this with your production URL
FRONTEND_ORIGIN=https://ceesarcode.example.com

# Executor Mode (docker or firecracker for production)
EXECUTOR_MODE=docker

# Logging
LOG_LEVEL=warn

# Feature Flags
ENABLE_PAIR_PROGRAMMING=true
ENABLE_SYSTEM_DESIGN_AGENT=true
ENABLE_WEB_SEARCH=true

# AI Provider Keys (production keys)
# GEMINI_API_KEY=
# OPENAI_API_KEY=
# ANTHROPIC_API_KEY=
DEFAULT_AI_PROVIDER=gemini

# Production Configuration
MAX_CONCURRENT_EXECUTIONS=50
EXECUTION_TIMEOUT_SECONDS=60
MAX_MEMORY_MB=512

